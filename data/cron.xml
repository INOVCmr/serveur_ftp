<odoo>
    <data noupdate="1">
        <record id="ir_cron_envoyer_fichier" model="ir.cron">
            <field name="name">Envoyer fichier FTP</field>
            <field name="model_id" ref="model_serveur_ftp"/>
            <field name="state">code</field>
            <field name="code">model.cron_envoyer_fichier()</field>
            <!-- <field name="schedule_type">minutes</field> -->
